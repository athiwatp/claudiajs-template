{
  "name": "_claudiajs-babel",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "files": [
    "bin"
  ],
  "scripts": {
    "transpile": "babel src --out-dir bin",
    "create": "yarn run transpile && yarn test && claudia create --region ap-northeast-1 --api-module bin/api",
